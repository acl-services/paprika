{
  "name": "@paprika/paprika",
  "description": "",
  "scripts": {
    "cypress:ci": "run-p --race storybook cypress:run",
    "cypress:dev": "run-p --race storybook cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "docz:setup": "cd documentation && yarn && yarn docz:dev",
    "docz:build": "cd documentation && yarn docz:build",
    "docz:dev": "cd documentation && yarn docz:dev",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint packages",
    "package": "./scripts/package/index.js",
    "prettier:format": "yarn prettier -- --write",
    "prettier": "prettier \"**/*.+(js|json|yml|css|scss|md|mdx)\"",
    "storybook": "start-storybook -p 9009 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o storybook-dist",
    "storybook:screener": "screener-storybook --conf screener.config.js",
    "jest": "jest --passWithNoTests",
    "a11y": "echo 'Axe will run on this command'",
    "tokens": "cd packages/tokens && yarn start"
  },
  "engines": {
    "yarn": ">=1.12.3"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.12",
    "@storybook/addon-knobs": "^4.1.12",
    "@storybook/addon-links": "^4.1.12",
    "@storybook/addons": "^4.1.12",
    "@storybook/react": "^4.1.12",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "css-loader": "^2.1.0",
    "cypress": "^3.1.5",
    "docz": "^1.0.4",
    "docz-theme-default": "^1.0.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "jest-dom": "^3.1.3",
    "lerna": "^3.13.0",
    "npm-run-all": "^4.1.5",
    "pa11y": "^5.0.4",
    "prettier": "^1.16.3",
    "react": "^16.8.2",
    "react-axe": "^3.0.1",
    "react-dom": "^16.8.2",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.0.3",
    "sass-loader": "^7.1.0",
    "screener-storybook": "^0.17.6",
    "sinon": "^7.2.4",
    "style-loader": "^0.23.1",
    "styled-components": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn jest && yarn prettier -- --list-different"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-dom/extend-expect",
      "react-testing-library/cleanup-after-each"
    ]
  }
}
