{
  "name": "@paprika/paprika",
  "description": "",
  "scripts": {
    "generateChangelog": "node scripts/changelogFiles.js",
    "cleanup": "yarn rimraf packages/*/lib",
    "cypress:ci": "run-p --race storybook cypress:run",
    "cypress:dev": "run-p --race storybook cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "wait-on http://localhost:9009 && cypress run --browser chrome",
    "jest:watch": "yarn jest --watch",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint packages --report-unused-disable-directives",
    "lint:jss": "stylelint './packages/**/src/**/*.styles.js'",
    "postpublish": "git add packages/*.md packages/*/index.d.ts && git commit -m 'Publish: automate readme, type definitions and changelog update' --no-verify && git push origin master -f --no-verify",
    "prepare": "yarn cleanup && lerna run pretranspile && yarn transpile && yarn generateReadMe && yarn generateTypeDefinitions",
    "prepublishOnly": "yarn generateChangelog",
    "prettier:format": "yarn prettier --write",
    "prettier": "prettier \"**/*.+(js|json|yml|css|scss)\"",
    "generateReadMe": "node scripts/readmeFiles.js",
    "registrymock:publish": "lerna publish --no-git-tag-version --no-push --registry http://localhost:4873",
    "registrymock:start": "verdaccio",
    "registrymock": "run-p -l registrymock:start registrymock:publish",
    "storybook:build": "build-storybook -s ./.storybook/public -c .storybook -o storybook-dist",
    "storybook:screener": "screener-storybook --conf screener.config.js",
    "storybook:start": "start-storybook -p 9009 -c .storybook",
    "storybook:uploadserver": "node scripts/uploaderServer",
    "storybook": "run-p storybook:start storybook:uploadserver",
    "test:a11y": "echo 'Axe will run on this command'",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "test": "jest --passWithNoTests",
    "transpile": "lerna exec --parallel -- babel --root-mode upward src --out-dir lib --source-maps --copy-files",
    "generateTypeDefinitions": "node scripts/typesFiles.js && yarn tsc"
  },
  "engines": {
    "yarn": ">=1.12.3"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@octokit/rest": "^16.28.6",
    "@storybook/addon-a11y": "^v6.0.0-beta.20",
    "@storybook/addon-actions": "^v6.0.0-beta.20",
    "@storybook/addon-cssresources": "v6.0.0-beta.20",
    "@storybook/addon-docs": "^v6.0.0-beta.20",
    "@storybook/addon-knobs": "^v6.0.0-beta.20",
    "@storybook/addon-links": "^v6.0.0-beta.20",
    "@storybook/addons": "^v6.0.0-beta.20",
    "@storybook/react": "^v6.0.0-beta.20",
    "@testing-library/cypress": "^4.0.5",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.3.0",
    "@testing-library/react-hooks": "^3.3.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "css-loader": "^2.1.0",
    "cypress": "3.3.1",
    "cypress-file-upload": "^4.0.7",
    "cypress-plugin-tab": "^1.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-better-styled-components": "^1.1.1",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "faker": "^4.1.0",
    "given2": "^2.1.7",
    "husky": "^2.5.0",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "js-yaml": "^3.13.1",
    "lerna": "^3.13.0",
    "lint-staged": "^8.2.1",
    "nanoid": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "pa11y": "^5.0.4",
    "prettier": "1.18.2",
    "react": "^16.8.2",
    "react-docgen": "^5.3.0",
    "react-dom": "^16.8.2",
    "react-element-to-jsx-string": "^14.3.1",
    "react-is": "^16.11.0",
    "react-resizable": "^1.10.1",
    "react-syntax-highlighter": "^12.2.1",
    "react-test-renderer": "^16.8.6",
    "rimraf": "3.0.2",
    "sass-loader": "^7.1.0",
    "screener-storybook": "^0.19.2",
    "shelljs": "^0.8.3",
    "sinon": "^7.2.4",
    "style-loader": "^0.23.1",
    "styled-components": "^4.2.0",
    "stylelint": "^13.1.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "typescript": "^4.0.2",
    "uuid": "^7.0.2",
    "verdaccio": "^3.11.6",
    "wait-on": "^3.2.0",
    "workerize-loader": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn jest"
    }
  },
  "lint-staged": {
    "packages/**/*.js": [
      "yarn lint",
      "yarn lint:jss"
    ],
    "packages/**/*.{js,json,yml,css,scss}": [
      "yarn prettier --check"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.4",
    "react-json-tree": "^0.11.2"
  }
}
