{
  "name": "@paprika/paprika",
  "description": "",
  "scripts": {
    "cypress:ci": "run-p --race storybook cypress:run",
    "cypress:dev": "run-p --race storybook cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "wait-on http://localhost:9009 && cypress run --browser chrome",
    "jest:watch": "yarn jest --watch",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint packages --report-unused-disable-directives",
    "prepare": "lerna run pretranspile && yarn transpile",
    "prettier:format": "yarn prettier --write",
    "prettier": "prettier \"**/*.+(js|json|yml|css|scss|md|mdx)\"",
    "registrymock:publish": "lerna publish --no-git-tag-version --no-push --registry http://localhost:4873",
    "registrymock:start": "verdaccio",
    "registrymock": "run-p -l registrymock:start registrymock:publish",
    "storybook:build": "build-storybook -c .storybook -o storybook-dist",
    "storybook:screener": "screener-storybook --conf screener.config.js",
    "storybook": "start-storybook -p 9009 -c .storybook",
    "test:a11y": "echo 'Axe will run on this command'",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "test": "jest --passWithNoTests",
    "transpile": "lerna exec --parallel -- babel --root-mode upward src --out-dir lib --source-maps --copy-files"
  },
  "engines": {
    "yarn": ">=1.12.3"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@octokit/rest": "^16.28.6",
    "@paprika/button": "^0.1.0",
    "@paprika/date-picker": "^0.3.1",
    "@paprika/heading": "^0.1.0",
    "@paprika/helpers": "^0.1.0",
    "@paprika/icon": "^0.1.0",
    "@paprika/input": "^0.1.0",
    "@paprika/l10n": "^0.1.0",
    "@paprika/listbox": "^0.5.0",
    "@paprika/popover": "^0.1.0",
    "@paprika/raw-button": "^0.1.0",
    "@paprika/select": "^0.1.0",
    "@paprika/sortable": "^0.1.0",
    "@paprika/spinner": "^0.1.0",
    "@paprika/stylers": "^0.1.0",
    "@paprika/textarea": "^0.1.1",
    "@paprika/tokens": "^0.1.0",
    "@storybook/addon-actions": "^v5.2.0-beta.19",
    "@storybook/addon-knobs": "^v5.2.0-beta.19",
    "@storybook/addon-links": "^v5.2.0-beta.19",
    "@storybook/addons": "^v5.2.0-beta.19",
    "@storybook/react": "^v5.2.0-beta.19",
    "@testing-library/cypress": "^4.0.3",
    "@testing-library/react": "^8.0.4",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "css-loader": "^2.1.0",
    "cypress": "3.3.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-better-styled-components": "^1.1.1",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.5.0",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "jest-dom": "^3.1.3",
    "js-yaml": "^3.13.1",
    "lerna": "^3.13.0",
    "lint-staged": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "pa11y": "^5.0.4",
    "prettier": "1.18.2",
    "react": "^16.8.2",
    "react-axe": "^3.0.1",
    "react-dom": "^16.8.2",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "screener-storybook": "^0.18.7",
    "sinon": "^7.2.4",
    "style-loader": "^0.23.1",
    "styled-components": "^4.2.0",
    "verdaccio": "^3.11.6",
    "wait-on": "^3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn jest"
    }
  },
  "lint-staged": {
    "packages/**/*.js": [
      "yarn lint"
    ],
    "packages/**/*.{js,json,yml,css,scss,md,mdx}": [
      "yarn prettier --check"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.4",
    "react-json-tree": "^0.11.2"
  }
}
